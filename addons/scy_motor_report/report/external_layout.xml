<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="external_layout_standard" inherit_id="web.external_layout_standard">

        <div t-attf-class="header o_company_#{company.id}_layout" position="replace">
            <div class="header">

                <div class="row"  >

                    <img src="/scy_motor_report/static/src/image/entete.jpg"
                         style="width: 90%; height: 90%; object-fit: cover;"
                           alt="Airport"/>

                </div>

            </div>
        </div>

        <div t-attf-class="footer o_standard_footer o_company_#{company.id}_layout" position="replace">
            <div class="footer o_standard_footer"  >
                <div>
                    <img src="/scy_motor_report/static/src/image/pieds.jpg"
                         style="width: 90%; height: 90%; object-fit: cover;"
                           alt="Airport"/>
                </div>
            </div>
        </div>

    </template>

</odoo>